  "scripts": {
    "start": "yarn copy_forks && yarn copy_tokens && NODE_OPTIONS=--openssl-legacy-provider && next start",
    "start_windows": "yarn copy_forks_windows && yarn copy_tokens_windows && set NODE_OPTIONS=--openssl-legacy-provider && next start",
    "build": "yarn copy_forks && yarn copy_tokens && NODE_OPTIONS=--openssl-legacy-provider && next build",
    "build_windows": "yarn copy_forks_windows && yarn copy_tokens_windows && set NODE_OPTIONS=--openssl-legacy-provider && next build",
    "dev": "yarn copy_forks && yarn copy_tokens && NODE_OPTIONS=--openssl-legacy-provider && next dev",
    "dev_windows": "yarn copy_forks_windows && yarn copy_tokens_windows && set NODE_OPTIONS=--openssl-legacy-provider && next dev",
    "start_prebuild": "http-server build -a localhost -p 3001 -o",
    "test": "next test --env=jsdom",
    "eject": "next eject",
    "postinstall": "yarn postinstall_linux",
    "postinstall_windows": "yarn copy_forks_windows",
    "postinstall_linux": "yarn copy_forks",
    "copy_tokens_windows": "xcopy /Y .\\forks\\@uniswap\\default-token-list\\build\\uniswap-default.tokenlist.json .\\public\\default.tokenlist.json",
    "copy_tokens": "cp ./forks/@uniswap/default-token-list/build/uniswap-default.tokenlist.json ./public/default.tokenlist.json",
    "copy_forks_windows": "xcopy /Y /E /I .\\forks\\@uniswap\\* .\\node_modules\\@uniswap",
    "copy_forks": "cp -r ./forks/@uniswap/ ./node_modules/",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.0",
